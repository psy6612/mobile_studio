<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <!--thin-->
    <font
        android:font="@font/notosanskrthin"
        app:font="@font/notosanskrthin"
        tools:ignore="UnusedAttribute"/>

    <!--light-->
    <font
        android:font="@font/notosanskrlight"
        app:font="@font/notosanskrlight"
        tools:ignore="UnusedAttribute"/>

    <!--demilight-->
    <font
        android:font="@font/notosanskrdemilight"
        app:font="@font/notosanskrdemilight"
        tools:ignore="UnusedAttribute"/>

    <!--regular-->
    <font
        android:font="@font/notosanskrregular"
        app:font="@font/notosanskrregular"
        tools:ignore="UnusedAttribute"/>

    <!--medium-->
    <font
        android:font="@font/notosanskrmedium"
        android:fontStyle="normal"
        app:font="@font/notosanskrmedium"
        app:fontStyle="normal"
        tools:ignore="UnusedAttribute"/>

    <!--bold-->
    <font
        android:font="@font/notosanskrbold"
        app:font="@font/notosanskrbold"
        tools:ignore="UnusedAttribute"/>

    <!--black-->
    <font
        android:font="@font/notosanskrblack"
        app:font="@font/notosanskrblack"
        tools:ignore="UnusedAttribute"/>

</font-family>